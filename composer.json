{
    "type": "project",
    "name": "enderlab/marvin-core",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "ext-swoole": "*",
        "api-platform/doctrine-orm": "^4.2",
        "api-platform/symfony": "^4.2",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.18",
        "doctrine/doctrine-migrations-bundle": "^3.5",
        "doctrine/orm": "^3.5",
        "gesdinet/jwt-refresh-token-bundle": "*",
        "lexik/jwt-authentication-bundle": "^3.1",
        "nelmio/cors-bundle": "^2.5",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpstan/phpdoc-parser": "^2.3",
        "runtime/frankenphp-symfony": "^0.2.0",
        "simps/mqtt": "^2.0",
        "symfony/asset": "7.4.*",
        "symfony/console": "7.4.*",
        "symfony/doctrine-messenger": "7.4.*",
        "symfony/dotenv": "7.4.*",
        "symfony/expression-language": "7.4.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "7.4.*",
        "symfony/http-client": "7.4.*",
        "symfony/intl": "7.4.*",
        "symfony/json-streamer": "7.4.*",
        "symfony/lock": "7.4.*",
        "symfony/mailer": "7.4.*",
        "symfony/mime": "7.4.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "7.4.*",
        "symfony/object-mapper": "7.4.*",
        "symfony/process": "7.4.*",
        "symfony/property-access": "7.4.*",
        "symfony/property-info": "7.4.*",
        "symfony/runtime": "7.4.*",
        "symfony/scheduler": "7.4.*",
        "symfony/security-bundle": "7.4.*",
        "symfony/serializer": "7.4.*",
        "symfony/string": "7.4.*",
        "symfony/translation": "7.4.*",
        "symfony/twig-bundle": "7.4.*",
        "symfony/uid": "7.4.*",
        "symfony/validator": "7.4.*",
        "symfony/web-link": "7.4.*",
        "symfony/yaml": "7.4.*",
        "symfonycasts/micro-mapper": "^0.2.1",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "webmozart/assert": "^1.11"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Marvin\\": "src/",
            "EnderLab\\DddCqrsBundle\\": "libs/enderlab/ddd-cqrs-bundle/src",
            "EnderLab\\DddCqrsApiPlatformBundle\\": "libs/enderlab/ddd-cqrs-api-platform-bundle/src",
            "EnderLab\\DddCqrsMakerBundle\\": "libs/enderlab/ddd-cqrs-maker-bundle/src",
            "EnderLab\\MarvinManagerBundle\\": "libs/enderlab/marvin-manager-bundle/src",
            "EnderLab\\ToolsBundle\\": "libs/enderlab/tools-bundle/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarvinTests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "marvin:code-quality": [
            "./vendor/bin/ecs check",
            "bin/console lint:yaml config --parse-tags",
            "bin/console lint:container",
            "./vendor/bin/rector --dry-run"
        ],
        "marvin:fix-code-quality": [
            "./vendor/bin/ecs check --fix",
            "bin/console lint:yaml config --parse-tags",
            "bin/console lint:container",
            "./vendor/bin/rector",
            "./vendor/bin/phpunit"
        ],
        "marvin:deptrac": [
            "./vendor/bin/deptrac --config-file=deptrac.yaml analyze"
        ],
        "marvin:migration": [
            "bin/console doctrine:migrations:migrate"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.4.*",
            "docker": true
        }
    },
    "require-dev": {
        "deptrac/deptrac": "^4.2",
        "doctrine/doctrine-fixtures-bundle": "^3.7",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpstan/phpstan-webmozart-assert": "^2.0",
        "phpunit/phpunit": "^12.4",
        "rector/rector": "^2.1",
        "symfony/browser-kit": "7.4.*",
        "symfony/css-selector": "7.4.*",
        "symfony/debug-bundle": "7.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "7.4.*",
        "symfony/stopwatch": "7.4.*",
        "symfony/web-profiler-bundle": "7.4.*",
        "symplify/easy-coding-standard": "^12.6",
        "zenstruck/browser": "^1.9",
        "zenstruck/console-test": "^1.7",
        "zenstruck/foundry": "^2.7",
        "zenstruck/mailer-test": "^1.4",
        "zenstruck/messenger-test": "^1.11"
    },
    "repositories": []
}
