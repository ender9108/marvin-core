{
    "name": "enderlab/marvin-zigbee2mqtt-bundle",
    "description": "",
    "type": "symfony-bundle",
    "licence": "MIT",
    "version": "1.0.2",
    "require": {
        "ext-swoole": "*",
        "enderlab/ddd-cqrs-bundle": "1.*"
    },
    "scripts": {
        "post-package-install": [
            "php bin/console marvin:zigbee2mqtt:install"
        ],
        "post-package-update": [
            "php bin/console marvin:zigbee2mqtt:update"
        ],
        "pre-package-uninstall": [
            "php bin/console marvin:zigbee2mqtt:remove"
        ]
    },
    "autoload": {
        "psr-4": {
            "EnderLab\\Zigbee2mqttBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EnderLab\\Zigbee2mqttBundle\\Tests\\": "tests/"
        }
    }
}
